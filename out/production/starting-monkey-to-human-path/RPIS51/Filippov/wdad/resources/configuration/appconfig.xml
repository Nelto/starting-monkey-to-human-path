<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE appconfig SYSTEM "appconfig.dtd">
<appconfig>
    <rmi>
        <server>
            <registry>
                <createregistry>yes</createregistry>
                <registryaddress>localhost</registryaddress>
                <registryport>5000</registryport>
            </registry>
        </server>
        <client>
            <policypath>client.policy</policypath>
            <usecodebaseonly>no</usecodebaseonly>
        </client>
        <classprovider>http://www.yourhost.free.ru/cp/cp.jar</classprovider>
    </rmi>
    <datasource>
        <classname>com.mysql.jdbc.Driver</classname>
        <drivertype>jdbc:mysql</drivertype>
        <hostName>localhost</hostName>
        <port>3306</port>
        <DBName>notes</DBName>
        <user>root</user>
        <pass></pass>
    </datasource>
</appconfig>